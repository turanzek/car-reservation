<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		id="viewSettingsDialog"
		confirm=".onConfirmViewSettingsDialog">
		<filterItems>
			<ViewSettingsFilterItem
				id="filterItems"
				text="{i18n>filterName}"
				key="Status"
				multiSelect="false">
				<items>
					<ViewSettingsItem
						id="viewFilter1"
						text="{i18n>masterFilterReserved}"
						key="Reserved"/>
					<ViewSettingsItem
						id="viewFilter2"
						text="{i18n>masterFilterNotReserved}"
						key="NotReserved"/>
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
		<groupItems>
			<ViewSettingsItem
				text="{i18n>masterGroupModel}"
				key="Model"/>
			<ViewSettingsItem
				text="{i18n>masterGroupVehicleyear}"
				key="Vehicleyear"/>
			<ViewSettingsItem
				text="{i18n>masterGroupFiyat}"
				key="Fiyat"/>
			<ViewSettingsItem
				text="{i18n>masterGroupReserved}"
				key="Reserved"/>
		</groupItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>